	    *(.gnu.linkonce.*)
	    *(__libc*)
	    *(.rel.dyn)
		_ecode = ABSOLUTE(.) ;	/* End of code				*/
		
		*(.rodata*)
		
		. = ALIGN(8);
		_etext = ABSOLUTE(.);	/* End of code and read-only data	*/
	} > ram
	
	.mdata :
