	.bss :
	{
		*(.bss)
		*(COMMON)
		. = ALIGN(8) ;
		_freemem = .;
	}
	_end_bss = . ;			/* End of unitialised data		*/

	.gptab.sdata : 
	{ 
		*(.gptab.data)
		*(.gptab.sdata) 
	}
